<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <h1>Lista de Posts</h1>
    <!-- Mostrar los posts aquÃ­ -->
    <ul>
      <% for (const post of posts) { %>
      <li>
        <p><%= post.id %></p>
        <h2><%= post.title %></h2>
        <p><%= post.description%></p>
        <p><%= post.author %></p>
      </li>
      <% } %>
    </ul>
  </body>
</html>
